{% extends "base.twig" %}

{% block content %}

	<div class="content-wrapper">
		<article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
			<section class="article-content">
				<h1 class="article-h1">Name: {{post.post_title}}</h1>
                <p>{{ post.get_field('jobtitle') }}</p>
                <p>{{ lang_title }}</p>

				{{post.get_content}}
			</section>
		</article>
	</div> <!-- /content-wrapper -->

{% endblock %}