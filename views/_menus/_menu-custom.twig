<ul>
    {% for item in menu_custom.get_items %}
        <li {{ item.classes | join(' ') }}"><a href="{{ item.get_link }}">{{ item.title }}</a>
        {% if item.get_children %}
            <ul>
                {% for child in item.get_children %}
                    <li><a href="{{ child.get_link }}">{{ child.title }}</a></li>
                {% endfor %}
            </ul>
        {% endif %}
        </li>
    {% endfor %}
</ul>