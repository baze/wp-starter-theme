{
  "name": "WPStarterTheme",
  "version": "0.1.0",
  "description": "WP starter Theme with common tasks and scenarios",
  "repository": {},
  "main": "./dest/js/main.js",
  "browser": {
    "modernizr": "./bower_components/modernizr/modernizr.js",
    "respond": "./bower_components/respond/dest/respond.min.js",
    "svgmagic": "./bower_components/SVGMagic/src/jquery.svgmagic.js",
    "responsiveNav": "./bower_components/responsive-nav/responsive-nav.min.js",
    "headroom": "./bower_components/headroom.js/dist/headroom.min.js"
  },
  "browserify": {
    "transform": [
      "debowerify",
      "deamdify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "modernizr": {
      "exports": "Modernizr"
    },
    "svgmagic": {
      "depends": [
        "jquery:jQuery"
      ]
    },
    "responsiveNav": {
      "exports": "responsiveNav"
    },
    "headroom": {
      "exports": "Headroom"
    }
  },
  "devDependencies": {
    "browser-sync": "^1.3.7",
    "browserify": "^5.11.2",
    "browserify-shim": "^3.7.0",
    "deamdify": "^0.1.1",
    "debowerify": "^0.8.1",
    "gulp": "^3.8.8",
    "gulp-autoprefixer": "^1.0.1",
    "gulp-changed": "^1.0.0",
    "gulp-compass": "^1.3.1",
    "gulp-gzip": "0.0.8",
    "gulp-if": "^1.2.4",
    "gulp-imagemin": "^1.0.1",
    "gulp-insert": "^0.4.0",
    "gulp-minify-css": "^0.3.8",
    "gulp-ng-annotate": "^0.3.2",
    "gulp-ng-autobootstrap": "^0.1.0",
    "gulp-ngmin": "^0.3.0",
    "gulp-notify": "^1.6.0",
    "gulp-rename": "^1.2.0",
    "gulp-ruby-sass": "^0.7.1",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "^1.0.1",
    "gulp-util": "^3.0.1",
    "pretty-hrtime": "^0.2.1",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^1.0.2"
  },
  "dependencies": {
    "jquery": "^2.1.1"
  }
}
